  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beameio/beame-sdk/issues"
  },
  "homepage": "https://github.com/beameio/beame-sdk#readme",
  "dependencies": {
    "async": "^2.0.0-rc.6",
    "cli-table2": "0.2.0",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "jmespath": "^0.15.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "network": "^0.2.1",
    "node-rsa": "^0.4.0",
    "node-uuid": "^1.4.7",
    "npm-prefix": "^1.2.0",
    "pem": "^1.8.3",
    "request": "^2.72.0",
    "rimraf": "^2.5.3",
    "serve-index": "^1.8.0",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.6",
    "sprintf": "^0.1.5",
    "sync-request": "^3.0.1",
    "underscore": "^1.8.3",
    "x509": "^0.2.6"
  },
  "directories": {
    "test": "tests"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-jsdoc3": "^0.3.0",
    "jsdoc": "^3.4.0",
    "mocha": "^2.5.3"
  }
