{
  "name": "beame-sdk",
  "version": "1.1.17",
  "description": "Beame.io SDK - x509 certificates and tunnelling traffic to your servers",
  "main": "index.js",
  "engine": "node >= 4.4.0",
  "scripts": {
    "test_developer": "mocha ./tests/unit-tests/single_tests/developer_test.js",
    "test_atom": "mocha ./tests/unit-tests/single_tests/atom_test.js",
    "test_edge": "mocha ./tests/unit-tests/single_tests/edge_test.js",
    "test_full_flow": "mocha ./tests/unit-tests/full_flow_test.js",
    "startChat": "node ./examples/example1.js",
    "runAtomServer": "node ./tests/serge_test/test_cli.js",
    "postinstall": "npm config get prefix | awk '{print \"node \"$0\"/lib/node_modules/beame-sdk/tests/server_tests/initRemoteEdgeClient.js\"}' | sh"
  },
  "bin": {
    "beame": "src/cli/beame.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:beameio/beame-sdk.git"
  },
  "keywords": [
    "beame",
    "SSL cert",
    "x509",
    "localhost",
    "proxy tunnel",
    "ssl tunnel",
    "self signed ",
    "CA",
    "signing",
    "crypto"
  ],
